{
	"name": "Ubuntu",
	"build": {
		"dockerfile": "Dockerfile",
		"args": {
			"VARIANT": "ubuntu-22.04"
		}
	},
	"postCreateCommand": "./.devcontainer/postCreateCommand.sh",
	"remoteUser": "vscode",
	"features": {
		"node": {
			"version": "lts",
			"nodeGypDependencies": true
		}
	},
	"customizations": {
		"vscode": {
			"extensions": [
				"Vue.volar",
				"esbenp.prettier-vscode"
			],
			"settings": {
				"editor.defaultFormatter": "esbenp.prettier-vscode"
			}
		}
	}
}